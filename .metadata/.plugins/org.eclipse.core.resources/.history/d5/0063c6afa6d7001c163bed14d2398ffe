#include <avr/io.h>
#include <util/delay.h>
#include "HAL/LCD/LCD_Interface.h"
#include "HAL/LCD/LCD_Configration.h"


int main(void)
{
	DDRA = 0xff;
	DDRB = 0xff;

	LCD_Init();
	LCD_SendIntNumb(1);
		_delay_ms(5000);
		LCD_CLEAR;
LCD_CLEAR;

while(1){


}

return 0;

}
