#include "LIB/STD_TYPES.h"
#include <avr/io.h>
#include <util/delay.h>
#include "HAL/LCD/LCD_Interface.h"
#include "HAL/LCD/LCD_Configration.h"


int main(void) {
	int pacMan1[8] = {
			0b00000000,
			0b00000100,
			0b00011111,
			0b00011110,
			0b00011100,
			0b00001110,
			0b00000100,
			0b00000000
	};
LCD_Init();
LCD_WriteToCGRAM(0,pacMan1 );

	while (1)
	{

	}

	return 0;
}
